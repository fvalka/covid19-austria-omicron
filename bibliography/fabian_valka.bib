@article{makEvaluationRapidAntigen2020,
  title = {Evaluation of Rapid Antigen Test for Detection of {{SARS-CoV-2}} Virus},
  author = {Mak, Gannon CK and Cheng, Peter KC and Lau, Stephen SY and Wong, Kitty KY and Lau, Cs and Lam, Edman TK and Chan, Rickjason CW and Tsang, Dominic NC},
  year = {2020},
