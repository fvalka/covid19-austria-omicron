  author = {{Simbana-Rivera}, Katherine and {Gomez-Barreno}, Lenin and Guerrero, Jhon and {Simbana-Guaycha}, Fernanda and Fernandez, Raul and {Lopez-Cortes}, Andres and Lister, Alex and {Ortiz-Prado}, Esteban},
